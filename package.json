{
  "name": "fuserewards",
  "version": "1.0.0",
  "description": "A simple template to use vue with phonegap",
  "main": "index.js",
  "scripts": {
    "watchify": "watchify -vd -p browserify-hmr -e www/src/main.js -o www/dist/build.js",
    "serve": "phonegap serve --port 3000",
    "dev": "npm-run-all --parallel watchify serve",
    "export_android": "export ANDROID_HOME=/home/fs20163/Android/Sdk && export PATH=${PATH}:$ANDROID_HOME/tools:$ANDROID_HOME/platform-tools && export PATH=$PATH:$ANDROID_HOME/bin",
    "build": "npm-run-all export_android && cross-env NODE_ENV=production & cordova build --release --device --target=",
    "compress": "cross-env NODE_ENV=production browserify -e www/src/main.js | uglifyjs -c -b warnings=false -m > www/dist/build.js",
    "test": "karma start karma.conf.js",
    "prepare": "cordova platform add",
    "ios": "cordova run ios",
    "android": "cordova run android",
    "compile": "browserify -e www/src/main.js -o www/dist/build.js & cordova compile --debug --target=",
    "clean:platforms": "rimraf platforms/*",
    "clean:dist": "rimraf www/dist/*",
    "clean": "npm-run-all clean:platforms clean:dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OmarGM2294/phonegap_simple_vuejs.git"
  },
  "keywords": [
    "cordova:template",
    "phonegap:template",
    "Phonegap",
    "Vue",
    "cordova"
  ],
  "browserify": {
    "transform": [
      "babelify",
      "vueify"
    ]
  },
  "browser": {
    "vue": "vue/dist/vue.common.js"
  },
  "author": "Omar Gonzalez",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/OmarGM2294/phonegap_simple_vuejs/issues"
  },
