<?xml version='1.0' encoding='utf-8'?>
<widget id="com.davshn.twenti" version="1.1.0" xmlns="http://www.w3.org/ns/widgets" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Twenti</name>
    <description>
    </description>
    <author email="davshn@gmail.com" href="https://github.com/davshn">
      David Figueroa
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-navigation href="*" />
    <allow-navigation href="http://*/*" />
    <allow-navigation href="https://*/*" />
    <allow-navigation href="https://srv-back-20181017.karrott.cl/*" />
    <allow-navigation href="https://srv-back-20181017.karrott.cl/api*" />
    <allow-navigation href="*://*srv-back-20181017.karrott.cl/*" />
    <allow-navigation href="*://*srv-back-20181017.karrott.cl/api*" />
    <allow-intent href="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="fb:*" />
    <allow-intent href="whatsapp:*" />
    <platform name="android">
        <access origin="*" />
        <allow-navigation href="*" />
        <allow-navigation href="http://*/*" />
        <allow-navigation href="https://*/*" />
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <preference name="android-build-tool" value="gradle" />
    <preference name="Orientation" value="portrait" />
    <platform name="browser">
        <hook src="scripts/build.js" type="before_serve" />
    </platform>
    <platform name="android">
        <icon density="mdpi" qualifier="mdpi" src="res/android/drawable-mdpi/icon.png" />
        <icon density="hdpi" qualifier="hdpi" src="res/android/drawable-hdpi/icon.png" />
        <icon density="xhdpi" qualifier="xhdpi" src="res/android/drawable-xhdpi/icon.png" />
        <icon density="xxhdpi" qualifier="xxhdpi" src="res/android/drawable-xxhdpi/icon.png" />
        <icon density="xxxhdpi" qualifier="xxxhdpi" src="res/android/drawable-xxxhdpi/icon.png" />
        <splash qualifier="land-ldpi" src="res/android/drawable-land-ldpi/screen.png" />
        <splash qualifier="land-mdpi" src="res/android/drawable-land-mdpi/screen.png" />
        <splash qualifier="land-hdpi" src="res/android/drawable-land-hdpi/screen.png" />
        <splash qualifier="land-xhdpi" src="res/android/drawable-land-xhdpi/screen.png" />
        <splash qualifier="land-xxhdpi" src="res/android/drawable-land-xxhdpi/screen.png" />
        <splash qualifier="land-xxxhdpi" src="res/android/drawable-land-xxxhdpi/screen.png" />
        <splash qualifier="port-ldpi" src="res/android/drawable-ldpi/screen.png" />
        <splash qualifier="port-mdpi" src="res/android/drawable-mdpi/screen.png" />
        <splash qualifier="port-hdpi" src="res/android/drawable-hdpi/screen.png" />
        <splash qualifier="port-xhdpi" src="res/android/drawable-xhdpi/screen.png" />
        <splash qualifier="port-xxhdpi" src="res/android/drawable-xxhdpi/screen.png" />
        <splash qualifier="port-xxxhdpi" src="res/android/drawable-xxxhdpi/screen.png" />
        <preference name="SplashMaintainAspectRatio" value="true" />
        <preference name="SplashShowOnlyFirstTime" value="false" />
        <preference name="android-minSdkVersion" value="19" />
        <preference name="android-targetSdkVersion" value="28" />
        <resource-file src="google-services.json" target="/app/google-services.json" />
        <config-file parent="/manifest" target="AndroidManifest.xml">
            <uses-permission android:name="android.permission.ACCESS_LOCATION_EXTRA_COMMANDS" />
            <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
            <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
        </config-file>
        <preference name="android-build-tool" value="gradle" />
        <plugin name="cordova-plugin-qrscanner" spec="^3.0.1" />
    </platform>
    <platform name="ios">
        <icon height="180" platform="ios" src="res/iOS/AppIcon.appiconset/Icon_180x180.png" width="180" />
        <icon height="60" platform="ios" src="res/iOS/AppIcon.appiconset/Icon-20x20@3x.png" width="60" />
        <icon height="58" platform="ios" src="res/iOS/AppIcon.appiconset/Icon_29x29@2x.png" width="58" />
        <icon height="87" platform="ios" src="res/iOS/AppIcon.appiconset/Icon-29x29@3x.png" width="87" />
        <icon height="40" platform="ios" src="res/iOS/AppIcon.appiconset/Icon-20x20@2x.png" width="40" />
        <icon height="80" platform="ios" src="res/iOS/AppIcon.appiconset/Icon-40x40@2x.png" width="80" />
        <icon height="120" platform="ios" src="res/iOS/AppIcon.appiconset/Icon-40x40@3x.png" width="120" />
        <icon height="29" platform="ios" src="res/iOS/AppIcon.appiconset/Icon_29x29@1x.png" width="29" />
        <icon height="76" platform="ios" src="res/iOS/AppIcon.appiconset/Icon-76x76@1x.png" width="76" />
        <icon height="152" platform="ios" src="res/iOS/AppIcon.appiconset/Icon-76x76@2x.png" width="152" />
        <icon height="167" platform="ios" src="res/iOS/AppIcon.appiconset/Icon-83.5@2x.png" width="167" />
        <icon height="1024" platform="ios" src="res/iOS/AppIcon.appiconset/Icon-marketing-1024x1024.png" width="1024" />
        <splash height="480" platform="ios" src="res/iOS/LaunchImage.launchimage/LaunchImage~iphone-320x480.png" width="320" />
        <splash height="960" platform="ios" src="res/iOS/LaunchImage.launchimage/LaunchImage~iphone_640x960.png" width="640" />
        <splash height="1136" platform="ios" src="res/iOS/LaunchImage.launchimage/LaunchImage-568h@2x~iphone_640x1136.png" width="640" />
        <splash height="1334" platform="ios" src="res/iOS/LaunchImage.launchimage/LaunchImage-750@2x~iphone6-portrait_750x1334.png" width="750" />
        <splash height="2208" platform="ios" src="res/iOS/LaunchImage.launchimage/LaunchImage-1242@3x~iphone6s-portrait_1242x2208.png" width="1242" />
        <splash height="1024" platform="ios" src="res/iOS/LaunchImage.launchimage/LaunchImage-Portrait~ipad_768x10244.png" width="768" />
        <splash height="768" platform="ios" src="res/iOS/LaunchImage.launchimage/LaunchImage-Landscape~ipad_1024x768.png" width="1024" />
        <splash height="2048" platform="ios" src="res/iOS/LaunchImage.launchimage/LaunchImage-Portrait@2x~ipad_1536x2048.png" width="1536" />
        <splash height="1536" platform="ios" src="res/iOS/LaunchImage.launchimage/LaunchImage-Landscape@2x~ipad_2048x1536.png" width="2048" />
        <plugin name="cordova-plugin-statusbar" spec="~2.1.3" />
        <preference name="StatusBarOverlaysWebView" value="false" />
        <preference name="StatusBarBackgroundColor" value="#000000" />
        <preference name="StatusBarStyle" value="lighttranslucent" />
        <resource-file src="GoogleService-Info.plist" />
        <plugin name="cordova-plugin-barcodescanner" spec="^0.7.4" />
    </platform>
    <icon height="1024" platform="ios" src="res/iOS/AppIcon.appiconset/Icon-marketing-1024x1024.png" width="1024" />
    <preference name="phonegap-version" value="cli-8.1.1" />
    <preference name="android-build-tool" value="gradle" />
    <gap:config-file parent="UIViewControllerBasedStatusBarAppearance" platform="ios">
        <truw />
    </gap:config-file>
    <edit-config file="*-Info.plist" mode="merge" platform="ios" target="NSCameraUsageDescription">
        <string>We are using the Camera for scan QR Codes associated with the offer redeemed of the user</string>
    </edit-config>
    <edit-config file="*-Info.plist" mode="merge" target="NSLocationWhenInUseUsageDescription">
        <string>deseamos acceder a tus datos de localización para darte mejores ofertas</string>
    </edit-config>
    <edit-config file="*-Info.plist" mode="merge" target="NSPhotoLibraryUsageDescription">
        <string>We are using the Photo Library for QR Codes scan associated with the offer redeemed of the user</string>
    </edit-config>
    <config-file parent="NSLocationWhenInUseUsageDescription" target="*-Info.plist">
        <string>deseamos acceder a tus datos de localización para darte mejores ofertas</string>
    </config-file>
    <plugin name="cordova-plugin-dialogs" spec="^2.0.1" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
    <plugin name="cordova-plugin-inappbrowser" spec="^3.0.0" />
    <plugin name="cordova-plugin-console" spec="^1.1.0" />
    <plugin name="cordova-plugin-device" spec="^2.0.2" />
    <plugin name="cordova-plugin-splashscreen" spec="^5.0.2" />
    <plugin name="cordova-plugin-statusbar" spec="^2.4.2" />
    <plugin name="cordova-plugin-facebook4" spec="4.2.1">
        <variable name="APP_ID" value="2128577704127876" />
        <variable name="APP_NAME" value="Sportlife" />
        <variable name="FACEBOOK_HYBRID_APP_EVENTS" value="false" />
        <variable name="FACEBOOK_ANDROID_SDK_VERSION" value="4.40.0" />
    </plugin>
    <plugin name="phonegap-plugin-push" spec="^2.2.3" />
    <plugin name="cordova-plugin-camera" spec="^4.0.3" />
    <plugin name="cordova-android-support-gradle-release" spec="^3.0.0" />
    <plugin name="cordova-plugin-geolocation" spec="^4.0.2" />
    <plugin name="cordova-plugin-brightness" spec="^0.2.0" />
    <engine name="browser" spec="~5.0.4" />
</widget>
